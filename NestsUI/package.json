{
  "name": "nestsui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "intl-extract": "yarn formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang.json --flatten true",
    "intl-compile": "yarn formatjs compile src/lang.json --out-file src/translations/en_US.json",
    "pre:commit": "yarn intl-extract && yarn intl-compile && yarn prettier --write ."
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.15",
    "@livekit/components-styles": "^1.1.6",
    "@livekit/protocol": "^1.43.0",
    "@scure/base": "^2.0.0",
    "@snort/shared": "^2.0.0-pre.3",
    "@snort/system": "^2.0.0-pre.8",
    "@snort/system-react": "^2.0.0-pre.8",
    "@snort/system-wasm": "^1.0.6",
    "@snort/wallet": "^2.0.0-pre.8",
    "@snort/worker-relay": "^2.0.0-pre.1",
    "babel-plugin-formatjs": "^10.5.41",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "ical-generator": "^10.0.0",
    "livekit-client": "^2.15.15",
    "qr-code-styling": "^1.9.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-intl": "^7.1.14",
    "react-router-dom": "^7.9.5",
    "react-swipeable": "^7.0.2",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.7.4",
    "@tailwindcss/vite": "^4.1.17",
    "@types/dom-mediacapture-record": "^1.0.22",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@webbtc/webln-types": "^3.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0"
  },
  "packageManager": "yarn@4.11.0",
  "prettier": {
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 2
  }
}
