{
  "name": "nestsui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "intl-extract": "yarn formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang.json --flatten true",
    "intl-compile": "yarn formatjs compile src/lang.json --out-file src/translations/en_US.json"
  },
  "dependencies": {
    "@livekit/components-react": "^1.5.3",
    "@livekit/components-styles": "^1.0.9",
    "@scure/base": "^1.1.5",
    "@snort/shared": "^1.0.13",
    "@snort/system": "^1.2.9",
    "@snort/system-react": "^1.2.9",
    "@snort/system-wasm": "^1.0.2",
    "babel-plugin-formatjs": "^10.5.13",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "livekit-client": "^1.15.10",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2",
    "react-router-dom": "^6.21.3",
    "tslib": "^2.6.2",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@webbtc/webln-types": "^3.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "packageManager": "yarn@4.0.2",
  "prettier": {
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 2
  }
}
